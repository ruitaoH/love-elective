@font-face
  font-family: 'elective-icon'
  src:  url('../fonts/elective-icon.eot?lzp6bm')
  src:  url('../fonts/elective-icon.eot?lzp6bm#iefix') format('embedded-opentype'),
    url('../fonts/elective-icon.ttf?lzp6bm') format('truetype'),
    url('../fonts/elective-icon.woff?lzp6bm') format('woff'),
    url('../fonts/elective-icon.svg?lzp6bm#elective-icon') format('svg')
  font-weight: normal
  font-style: normal

[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'elective-icon' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

.icon-clear:before
  content: "\e903"

.icon-search:before
  content: "\e902"

.icon-circle-right:before
  content: "\e901"

.icon-thumb_up:before
  content: "\e900"

.icon-close:before
  content: "\e906"

.icon-arrow_lift:before
  content: "\e908"

